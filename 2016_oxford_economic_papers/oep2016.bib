  Keywords                 = {Food crisis,Food insecurity measurement,Food policy,Sub-Saharan Africa},
  Owner                    = {swl},
  Timestamp                = {2014.11.05},
  Url                      = {http://dx.doi.org/10.1016/j.foodpol.2012.12.006}
}

